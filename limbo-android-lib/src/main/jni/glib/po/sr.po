#, c-format
msgid "'%s' is not a valid name "
msgstr "„%s“ није исправно име "

#: ../glib/gmarkup.c:389
#, c-format
msgid "'%s' is not a valid name: '%c' "
msgstr "„%s“ није исправно име: „%c“ "

